    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lib-flexible": "^0.3.2",
    "postcss-pxtorem": "^4.0.1",
    "vconsole": "^3.7.0",
    "vue-template-compiler": "^2.6.10"
